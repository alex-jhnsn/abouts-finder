{
  "name": "All Abouts Finder",
  "description": "Find and copy all about links to add to your stories.",
  "version": "1.0",
  "homepage_url": "https://github.com/alex-jhnsn/abouts-finder",
  "manifest_version": 3,
  "icons": { 
    "16": "/images/icon16.png",
    "48": "/images/icon48.png",
   "128": "/images/icon128.png" },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+L",
        "windows": "Alt+L",
        "mac": "Alt+L",
        "chromeos": "Alt+L",
        "linux": "Alt+L"
      },
      "description": "Search for a link."
    }
  },
  "options_page": "options.html",
  "permissions": ["scripting", "storage", "unlimitedStorage"]
}